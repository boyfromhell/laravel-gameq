{
    "name": "ichbin/gameq",
    "description": "GameQ Gameserver Library for Laravel",
    "keywords": [
        "laravel",
        "gameq",
        "serverstatus"
    ],
    "version": "v1",
    "homepage": "https://github.com/boyfromhell/laravel-gameq",
    "license": "LGPL-3.0+",
    "type": "library",
    "authors": [
        {
            "name": "BoyFromHell",
            "email": "boyfromhell86@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Austin Bischoff",
            "email": "austin.bischoff@gmail.com",
            "role": "Packagist/Composer Maintainer, Developer",
            "homepage": "https://github.com/Austinb"
        }
    ],
    "support": {
        "source": "https://github.com/Austinb/GameQ/v3",
        "issues": "https://github.com/Austinb/GameQ/issues",
        "wiki": "https://github.com/Austinb/GameQ/wiki"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-bz2": "*",
        "ext-xml": "*",
        "illuminate/support": "^8.0",
        "austinb/gameq": "3.*@stable"
    },
    "require-dev": {
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "IchBin\\GameQ\\": "src"
        }
    },
    "autoload-dev": {},
    "scripts": {},
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IchBin\\GameQ\\LaravelGameQServiceProvider"
            ],
            "aliases": {
                "GameQ": "IchBin\\GameQ\\LaravelGameQFacade"
            }
        }
    }
}
